# Urxvt settings:
# von http://gear1924.wordpress.com/2009/09/28/tabs-mit-urxvt/
URxvt.saveLines: 10000
URxvt.scrollBar: false
URxvt.foreground: gray
URxvt.background: black
! for screen
URxvt.secondaryScroll: true

! make links clickable
URxvt.perl-ext-common: default,matcher
URxvt.urlLauncher: chromium
#URxvt.urlLauncher: chromium
#URxvt.urlLauncher: firefox
URxvt.matcher.button: 1
URxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
#URxvt.perl-ext-common: mark-urls
#URxvt.urlLauncher: firefox

! tabbed
Urxvt.tabbed: true
URxvt.tabbed.saveLines: 10000
#URxvt.tabbed.scrollBar: true
URxvt.tabbed.tabbar-fg: 3
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 0
URxvt.tabbed.tab-bg: 1
#URxvt.tabbed.secondaryScroll: true
#URxvt.tabbed.perl-ext-common: mark-urls
#URxvt.tabbed.urlLauncher: firefox 

! real transparency
#URxvt.inheritPixmap: true
#URxvt.tintColor: red
#URxvt.shading: 20

